{
	"name": "sso",
	"displayName": "sso",
	"description": "快手商业化前端VSCODE-SSO插件",
	"version": "1.0.12",
	"publisher": "Kcfe",
	"engines": {
		"vscode": "^1.50.0"
	},
	"keywords": [
		"React",
		"Kuaishou AD FE",
		"KCFE",
		"JavaScript",
		"TypeScript",
		"Visual",
		"FE Tools"
	],
	"activationEvents": [
		"onView:sso.dev.panel"
	],
	"icon": "assets/images/sso.jpg",
	"main": "./dist/extension.js",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "sso-activitybar",
					"title": "sso",
					"icon": "assets/images/sso.jpg",
					"when": "sso.sub.show"
				}
			]
		},
		"views": {
			"sso-activitybar": [
				{
					"id": "sso.dev.panel",
					"type": "webview",
					"name": "sso",
					"icon": "src/extension/assets/images/sso.jpg",
					"contextualTitle": "sso",
					"when": "sso.sub.show"
				}
			]
		},
		"commands": [],
		"snippets": [
			{
				"language": "typescriptreact",
				"path": "./snippets/muijson.json"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "webpack",
		"watch": "webpack --watch",
		"package": "webpack --mode production --devtool hidden-source-map",
		"test-compile": "tsc -p ./",
		"test-watch": "tsc -watch -p ./src/extension/*",
		"pretest": "npm run test-compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"install-ui-deps": "cd ./src/ui && npm i",
		"build-ui": "cd ./src/ui && npm run build",
		"watch-build-ui": "cd ./src/ui && npm run watch-build"
	},
	"dependencies": {
		"@ad/postmessage-client": "^1.0.1",
		"@ad/postmessage-server": "1.0.2-beta.10",
		"@types/puppeteer-core": "^5.4.0",
		"puppeteer-core": "^14.1.1",
		"ramda": "^0.27.1",
		"utils": "0.0.1",
		"vscode-jsonrpc": "^4.0.0",
		"axios": "^0.25.0",
		"jszip": "^3.10.0"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.2.2",
		"@types/node": "14.x",
		"@types/utils": "0.0.1",
		"@types/vscode": "^1.50.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.26.0",
		"eslint": "^7.27.0",
		"glob": "^7.1.7",
		"mocha": "^8.4.0",
		"ts-loader": "^9.2.2",
		"typescript": "^4.3.2",
		"vscode": "^1.1.37",
		"vscode-test": "^1.5.2",
		"webpack": "^5.38.1",
		"webpack-cli": "^4.7.0"
	},
